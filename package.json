{
  "name": "react-flux-routing-seo",
  "version": "0.0.1",
  "description": "React/Flux/React-Router SEO goodness",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && node server.js",
    "start-dev": "npm run watch & npm start",
    "build": "browserify -t reactify app.js > static/bundle.js",
    "watch": "watchify -t reactify app.js lib/**/*.* -o static/bundle.js -dv"
  },
  "author": "Brian Schemp",
  "dependencies": {
    "ecstatic": "3.2.0",
    "ejs": "^2.2.2",
    "es6-promise": "^2.0.1",
    "flux": "^2.0.1",
    "node-jsx": "^0.12.4",
    "object-assign": "^2.0.0",
    "react": "^0.13.1",
    "react-router": "^0.13.2",
    "routes": "^2.0.0",
    "serialize-javascript": "^1.0.0",
    "superagent": "^0.21.0"
  },
  "devDependencies": {
    "browserify": "^9.0.7",
    "reactify": "^1.1.0",
    "watchify": "^3.1.0"
  }
}
